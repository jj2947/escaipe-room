<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<Pane fx:id="startScreen" prefHeight="470.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nz.ac.auckland.se206.controllers.StartScreenController">
   <children>
      <VBox alignment="TOP_CENTER" layoutY="14.0" prefHeight="27.0" prefWidth="600.0">
         <children>
            <Label fx:id="titleLabel" alignment="TOP_LEFT" text="WELCOME TO THE ESCAPE ROOM" />
         </children>
      </VBox>
      <VBox alignment="CENTER" layoutY="63.0" prefHeight="58.0" prefWidth="600.0">
         <children>
            <Label fx:id="titleLabel1" alignment="TOP_LEFT" text="You are stuck at school and are going to miss the only bus home, you better hurry" wrapText="true" />
            <Label fx:id="titleLabel11" alignment="TOP_LEFT" text="Select a Difficulty and Time" wrapText="true" />
         </children>
      </VBox>
      <Button fx:id="startButton" disable="true" layoutX="100.0" layoutY="370.0" mnemonicParsing="false" onAction="#onStart" text="START">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="exitButton" layoutX="440.0" layoutY="370.0" mnemonicParsing="false" onAction="#onExit" text="EXIT">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Label layoutX="49.0" layoutY="229.0" text="TIME IN MINUTES:" />
      <Label layoutX="64.0" layoutY="157.0" text="DIFFICULTY:" />
      <Button fx:id="easyButton" layoutX="159.0" layoutY="153.0" mnemonicParsing="false" onAction="#onEasy" text="EASY" />
      <Button fx:id="mediumButton" layoutX="240.0" layoutY="153.0" mnemonicParsing="false" onAction="#onMedium" text="MEDIUM" />
      <Button fx:id="hardButton" layoutX="341.0" layoutY="153.0" mnemonicParsing="false" onAction="#onHard" text="HARD" />
      <Button fx:id="twoButton" layoutX="171.0" layoutY="225.0" mnemonicParsing="false" onAction="#onTwo" text="2" />
      <Button fx:id="fourButton" layoutX="215.0" layoutY="225.0" mnemonicParsing="false" onAction="#onFour" text="4" />
      <Button fx:id="sixButton" layoutX="260.0" layoutY="225.0" mnemonicParsing="false" onAction="#onSix" text="6" />
   </children>
</Pane>
